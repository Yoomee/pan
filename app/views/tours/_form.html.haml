=semantic_form_for(@tour, :html => {:multipart => true}) do |form|
  =error_alert(@tour)
  =tabs('details', ["slideshow", "Media"], 'organisations/tabs/links', ['requirements', 'Technical requirements'], 'availability', 'external_reviews/tabs/reviews','pricing', :form => form) 
  
  =form.buttons do
    =form.commit_button "Save all show details", :button_html => {:class => 'btn-primary'}